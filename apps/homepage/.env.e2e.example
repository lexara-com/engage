# E2E Test Environment Variables
# Copy this file to .env.e2e and fill in your test credentials

# Playwright Configuration
PLAYWRIGHT_BASE_URL=https://dev.console.lexara.app

# Auth0 Test Credentials
# IMPORTANT: Use a dedicated test account, never production credentials
E2E_TEST_EMAIL=your-test-email@example.com
E2E_TEST_PASSWORD=your-test-password
E2E_TEST_FIRM_ID=test-firm-id

# Auth0 Configuration (optional, for direct API testing)
AUTH0_DOMAIN=your-tenant.auth0.com
AUTH0_CLIENT_ID=your-client-id
AUTH0_CLIENT_SECRET=your-client-secret
AUTH0_AUDIENCE=your-api-audience

# Test User Details (optional)
E2E_TEST_USER_NAME="Test Lawyer"
E2E_TEST_USER_ROLE=admin