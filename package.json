{
  "name": "engage-legal-ai",
  "version": "0.1.0",
  "description": "Engage - AI-powered legal client intake platform",
  "main": "src/agent/main-worker.ts",
  "scripts": {
    "dev": "wrangler dev",
    "dev:ui": "astro dev",
    "build:ui": "astro build",
    "preview:ui": "astro preview",
    "deploy": "wrangler deploy",
    "deploy:staging": "wrangler deploy --env staging",
    "deploy:production": "wrangler deploy --env production",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:e2e": "vitest run tests/e2e",
    "test:e2e:watch": "vitest watch tests/e2e",
    "test:e2e:debug": "DEBUG_TESTS=true vitest run tests/e2e",
    "test:e2e:headed": "HEADLESS=false vitest run tests/e2e",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix"
  },
  "devDependencies": {
    "@astrojs/node": "^8.3.4",
    "@astrojs/tailwind": "^5.1.2",
    "@cloudflare/workers-types": "^4.20240909.0",
    "@types/node": "^20.14.0",
    "@types/puppeteer": "^5.4.7",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "astro": "^4.16.7",
    "eslint": "^8.57.0",
    "puppeteer": "^24.10.0",
    "tailwindcss": "^3.4.14",
    "typescript": "^5.5.0",
    "vitest": "^1.6.0",
    "wrangler": "^3.78.0"
  },
  "dependencies": {
    "@astrojs/cloudflare": "^11.0.0",
    "ulid": "^2.3.0"
  },
  "keywords": [
    "legal-tech",
    "ai",
    "cloudflare-workers",
    "client-intake",
    "conflict-detection"
  ],
  "author": "Lexara",
  "license": "PROPRIETARY"
}
